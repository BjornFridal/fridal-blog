---
import ContainerOuter from './Containers/ContainerOuter.astro';
import ContainerInner from './Containers/ContainerInner.astro';

const links = [
  {
    url: '/',
    label: 'Blog'
  },
  {
    url: '/projects',
    label: 'Projects'
  },
  {
    url: '/about',
    label: 'About'
  }
];
---

<footer class="mt-32 flex-none">
  <ContainerOuter>
    <div class="border-t border-zinc-100 pb-16 pt-10">
      <ContainerInner>
        <div
          class="flex flex-col items-center justify-between gap-6 sm:flex-row"
        >
          <p class="text-sm text-zinc-400">
            &copy; {new Date().getFullYear()} Fridal. All rights reserved.
          </p>
        </div>
      </ContainerInner>
    </div>
  </ContainerOuter>
</footer>
